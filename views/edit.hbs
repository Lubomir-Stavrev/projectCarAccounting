
    
    <div class="create-Container">
        <div>
            <div class="page-Title">Edit Car</div>
            <div class="custom-Line"></div>

            <form  method="POST">

{{#if error}}
<div class="errorBox">
    <p>{{error.message}}</p>
</div>
{{/if}}

                <label for="fname">Owner</label><br>
  <input class="create-Inputs" type="text" value="{{owner}}" name="owner" ><br>

  <label for="lname">Year</label><br>
  <input class="create-Inputs" type="text" value="{{year}}" name="year" ><br>

  <label for="fname">Type</label><br>
